if (getUrlParameter("disc")) {

 var paramValue = getUrlParameter("disc");
 var disclosureArray = paramValue.split(",");
 var disclosureValues = [];

 for (var i = 0; i < disclosureArray.length; i++) {
       var evalDisclosureValue = addLeadingZeros(disclosureArray[i], 5);
       disclosureValues.push(evalDisclosureValue);
     }
 var disclosureUrlValues = disclosureValues.join(",");
 /// find url and insert values
 var newDiscValues = document.querySelector('.acf-disclosure');
 newDiscValues.setAttribute('href', "https://www.lendingtree.com/legal/advertising-disclosures?disclosures=" + disclosureUrlValues + '"');

}

function addLeadingZeros(num, size) {
     var s = num+"";
     while (s.length < size) s = "0" + s;
     return s;
 }

function getUrlParameter(name) {
    name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
    var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
    var results = regex.exec(location.search);
    return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
};    
