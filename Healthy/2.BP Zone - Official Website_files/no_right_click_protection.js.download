        $('#CybotCookiebotDialogBodyButtonAccept').click( function() {
            $('#CybotCookiebotDialog').fadeOut();                                                 
        });
    }
    //JSON Call
    var endpoint = 'https://pro.ip-api.com/json/?fields=status,message,countryCode,regionName&key=5vWn1Ta5eS1fVrD';
    var xhr = new XMLHttpRequest();
    xhr.onreadystatechange = function() {
        if (this.readyState == 4 && this.status == 200) {
            var response = JSON.parse(this.responseText);
            if(response.countryCode !== "US") {
                $('.USAcustomers').addClass('uk-hidden');
                $('.INTcustomers').removeClass('uk-hidden');
                if($('.shipping').length) {
                    $('.shipping:not(.shipping_free)').addClass('shipping_int').removeClass('font-bold');
                }
                $('#coronavirus_notice').replaceWith('<section uk-scrollspy="cls:uk-animation-slide-down;delay:1000" class="uk-background-muted"><div class="uk-container uk-container-xsmall uk-margin-remove-bottom" uk-alert><a class="uk-alert-close" uk-close></a><h4 class="uk-margin-small-bottom">COVID-19 Notice:</h4><p style="font-size:16px;margin-top:0">Due to COVID-19 we are experiencing shipment delays on <strong class="uk-text-bold">some International orders</strong>. Orders placed <u>outside of the U.S. only</u>, may take up to 4-6 weeks to arrive. We are so sorry for the inconvenience and we are doing everything we can to rush these orders.</p></div></section>');
                
                $('#shippingInformationTY').replaceWith('<div id="shippingInformationTY" class="uk-card-body uk-card-small uk-card uk-background-default uk-card-default"><h3 class="text-blue uk-text-center">Shipping Information</h3><hr><p class="uk-alert uk-text-justify uk-box-shadow-medium"><span class="uk-label uk-label-success">Domestic Orders</span><br>Your Supplement order is being shipped out today and it will arrive within the next 7-10 business days for orders within the United States.</p><p class="uk-alert uk-text-justify uk-alert-danger uk-box-shadow-medium"><span class="uk-label uk-label-danger">International Orders</span><br>Due to COVID-19 we are experiencing shipment delays on some International orders. Orders placed outside of the U.S. may take up to 4-6 weeks to arrive. We are so sorry for the inconvenience and we are doing everything we can to rush these orders.</p><hr><p class="uk-text-center"><em>Your items will arrive in a discreet white bubble mailer or box.</em></p></div>');
            }
            if(response.regionName == "California") {
                setTimeout(cookieBot, 5000);
            }
        }
    };
    xhr.open('GET', endpoint, true);
    xhr.send();
});
