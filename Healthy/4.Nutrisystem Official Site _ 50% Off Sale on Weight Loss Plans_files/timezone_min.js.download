function pad(B,A){var C=""+B;while(C.length<A){C="0"+C}return C}function getOffset(){var B=new Date().getTimezoneOffset();var A=(Math.abs(B/60)+"").split(".");B=((B<0?"+":"-")+pad(A[0],1)+":"+pad(Math.abs(B%60),2));return B}function setTimezone(){$.ajax({url:"/jsps_hmr/ajax/set_timezone.jsp",data:{tz:getOffset()},success:function(A){}})}function setTimezoneNsmembers(){$.ajax({url:"/nsmembers/jsps/common/set_timezone.jsp",data:{tz:getOffset()},success:function(A){}})};