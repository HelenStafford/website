function completeAndRedirect()
{
var phoneNumber = document.getElementById('phoneNumber1').innerHTML;
var phone = phoneNumber.replace(/\./g,"");
var campaignID = getUrlVars()['CAMPAIGN'];
if(campaignID == undefined){
 campaignID="";
 }
window.open("https://purchase.allstate.com/onlineshopping/welcome?Product=Au&TFN="+ phone+"&CAMPAIGN="+campaignID+"&intcid=ILC-LSP-auto-renterslz:GetAQuote ","_self");
}
